{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "ui-testing"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ]
    },
    "extends": [
        "plugin:ui-testing/playwright"
    ]
}